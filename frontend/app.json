{
  "expo": {
    "name": "Daily Training Engine",
    "slug": "daily-training-engine",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dailytraining",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dailytraining.engine"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0a0a0a"
      },
      "package": "com.dailytraining.engine"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "splash": {
      "image": "./assets/images/splash-image.png",
      "backgroundColor": "#0a0a0a",
      "resizeMode": "contain"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {}
    }
  }
}
